{% comment %}{% if site.data.social.rss or site.theme_settings.rss %}{% endcomment %}
{% comment %}<li>{% endcomment %}
    {% comment %}<a feed.xml href="{{ site.data.social.feed.path | default: 'feed.xml' | relative_url }}"{% endcomment %}
       {% comment %}title="{{ site.data.language.str_rss_follow | default: 'Follow RSS feed' }}"{% endcomment %}
       {% comment %}target="_blank">{% endcomment %}
        {% comment %}<span class="fa-stack fa-lg">{% endcomment %}
            {% comment %}<i class="fas fa-circle fa-stack-2x"></i>{% endcomment %}
            {% comment %}<i class="fas fa-rss fa-stack-1x fa-inverse"></i>{% endcomment %}
        {% comment %}</span>{% endcomment %}
    {% comment %}</a>{% endcomment %}
{% comment %}</li>{% endcomment %}
{% comment %}{% endif %}{% endcomment %}
{% comment %}{% endcomment %}
{% comment %}{% for footer in site.data.icons %}{% endcomment %}
    {% comment %}{% unless footer[1] == nil %}{% endcomment %}
        {% comment %}{% assign brand = footer[0] %}{% endcomment %}
        {% comment %}{% assign id = footer[1] %}{% endcomment %}
        {% comment %}{% assign data = site.data.icons_builder[brand] %}{% endcomment %}
        {% comment %}{% if data.icon == nil %}{% endcomment %}
            {% comment %}{% assign icon = "fab fa-" | append: brand %}{% endcomment %}
        {% comment %}{% else %}{% endcomment %}
            {% comment %}{% assign icon = data.icon %}{% endcomment %}
        {% comment %}{% endif %}{% endcomment %}
        {% comment %}<li>{% endcomment %}
            {% comment %}<a href="{{ data.pre | append: id | append: data.pos }}"{% endcomment %}
               {% comment %}title="{{ site.data.language.str_follow_on }} {{ brand | remove: '-' | capitalize }}"{% endcomment %}
               {% comment %}target="_blank"{% endcomment %}
               {% comment %}rel="noopener">{% endcomment %}
            {% comment %}<span class="fa-stack fa-lg">{% endcomment %}
              {% comment %}<i class="fas fa-circle fa-stack-2x"></i>{% endcomment %}
              {% comment %}<i class="{{ icon }} fa-stack-1x fa-inverse"></i>{% endcomment %}
            {% comment %}</span>{% endcomment %}
            {% comment %}</a>{% endcomment %}
        {% comment %}</li>{% endcomment %}
    {% comment %}{% endunless %}{% endcomment %}
{% comment %}{% endfor %}{% endcomment %}
